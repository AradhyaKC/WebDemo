@model IEnumerable<WebDemo.Models.WorksOnModel>

<h2>View Project Team Members</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.employeeId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.firstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.projectId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.role)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.shiftStartTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.shiftEndTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.employeeId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.firstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.projectId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.role)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.shiftStartTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.shiftEndTime)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditProjectEmployee", new { employeeId = item.employeeId,projectId = item.projectId })
        </td>
    </tr>
}
</table>

